{
  "name": "lw-v3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "prepare": "husky",
    "lint": "eslint ./src/**/*.{js,ts,vue}",
    "format": "prettier .  --write"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "npm run lint",
      "npm run format"
    ],
    "*.{html,scss,css}": [
      "npm run format"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.1",
    "@fortawesome/free-brands-svg-icons": "^6.7.1",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "gh-pages": "^6.2.0",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "postcss": "^8.4.49",
    "prettier": "3.4.2",
    "tailwindcss": "^3.4.16",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.0",
    "vite": "^6.0.1",
    "vue-tsc": "^2.1.10"
  }
}
